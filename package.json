{
  "name": "test-series-pro",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "build:android:preview": "npx eas-cli build --platform android --profile preview",
    "build:android:production": "npx eas-cli build --platform android --profile production",
    "submit:android:production": "npx eas-cli submit --platform android --profile production",
    "postinstall": "mkdir -p node_modules/react-native/node_modules/@react-native && ln -sf ../../../@react-native/normalize-colors node_modules/react-native/node_modules/@react-native/normalize-colors && mkdir -p node_modules/@supabase/functions-js/node_modules && ln -sf ../../../tslib node_modules/@supabase/functions-js/node_modules/tslib"
  },
  "overrides": {
    "@react-native/normalize-colors": "0.76.9"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/flash-list": "1.7.3",
    "@supabase/supabase-js": "^2.39.7",
    "expo": "~52.0.28",
    "expo-auth-session": "~6.0.3",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-iap": "^3.4.9",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-secure-store": "~14.0.1",
    "expo-speech": "~13.0.1",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.5",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-web": "~0.19.13",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "babel-preset-expo": "~12.0.2",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-av",
          "react-native-chart-kit"
        ]
      }
    },
    "install": {
      "exclude": [
        "expo-av",
        "react-native-chart-kit"
      ]
    }
  }
}
